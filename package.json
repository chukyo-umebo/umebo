{
  "name": "umebo",
  "main": "src/index.ts",
  "version": "0.0.1",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "format": "prettier --write .",
    "prebuild": "expo prebuild --clean",
    "devbuild:android": "eas build -p android --profile development --local",
    "devbuild:ios": "eas build -p ios --profile development --local",
    "build:android": "eas build -p android --profile production --output=./production.aab --local",
    "build:ios": "eas build -p ios --profile production --output=./production.ipa --local"
  },
  "dependencies": {
    "expo": "~54.0.32",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.22",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.39.2",
    "eslint-config-expo": "~10.0.0",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "~5.9.3"
  },
  "private": true,
  "trustedDependencies": [
    "unrs-resolver"
  ]
}
